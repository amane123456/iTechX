node n0 {
    type pc
    network-config {
	hostname pc1
	!
	interface eth0
	 ip address 10.0.4.20/24
	 ipv6 address fc00:4::20/64
	 mac address 42:00:aa:00:00:09
	!
	interface lo0
	 type lo
	 ip address 127.0.0.1/8
	 ipv6 address ::1/128
	!
	ip route 0.0.0.0/0 10.0.4.1
	!
	ipv6 route ::/0 fc00:4::1
	!
    }
    canvas c0
    iconcoords {840 168}
    labelcoords {840 199}
    interface-peer {eth0 n5}
}


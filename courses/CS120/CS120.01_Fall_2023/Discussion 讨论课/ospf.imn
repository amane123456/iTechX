    delay 5000
    nodes {n4 n2}
    bandwidth 1000000
}

link l1 {
    delay 5000
    nodes {n3 n2}
    bandwidth 1000
}

link l2 {
    delay 5000
    nodes {n3 n4}
    bandwidth 1000000
}

link l3 {
    nodes {n5 n3}
    bandwidth 0
}

link l4 {
    nodes {n0 n5}
    bandwidth 0
}

link l5 {
    nodes {n1 n4}
    bandwidth 0
}

canvas c0 {
    name {Canvas0}
}

option show {
    interface_names yes
    ip_addresses yes
    ipv6_addresses no
    node_labels yes
    link_labels yes
    background_images no
    annotations yes
    hostsAutoAssign no
    grid yes
    iconSize normal
    zoom 1.0
}

